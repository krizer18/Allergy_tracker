{
  "manifest_version": 3,
  "name": "Allergy Extension",
  "version": "1.0.0",
  "background": {
    "service_worker": "src/service-worker.js"
  },
  "icons" : {
    "64" : "icon.png"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "scripting"
    ],
  "host_permissions": [
    "<all_urls>"
    ]
}
